                    "fontSize": "16px"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "ModeDropdown"
                },
                "propConfig": {
                  "value": {
                    "binding": {
                      "type": "tag",
                      "config": {
                        "tagPath": "[default]YW_Demo/Mode/AutoSelected",
                        "bidirectional": true
                      }
                    }
                  }
                },
                "props": {
                  "options": [
                    {"value": true, "label": "Automatic"},
                    {"value": false, "label": "Manual"}
                  ],
                  "style": {
                    "minWidth": "200px"
                  }
                },
                "type": "ia.input.dropdown"
