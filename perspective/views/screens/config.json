                  {
                    "meta": {
                      "name": "Btn1"
                    },
                    "events": {
                      "component": {
                        "onActionPerformed": [
                          {
                            "scope": "session",
                            "script": "# Set tank count to 1\nsystem.tag.writeBlocking(['[default]YW_Demo/Config/TankCount'], [1])\n# Enable only tank 1, disable the rest\nsystem.tag.writeBlocking(\n\t['[default]YW_Demo/Tanks/Tank_1/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_2/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_3/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_4/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_5/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_6/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_7/Enabled',\n\t '[default]YW_Demo/Tanks/Tank_8/Enabled'],\n\t[True, False, False, False, False, False, False, False]\n)"
                          }
                        ]
                      }
                    },
                    "propConfig": {
                      "style": {
                        "binding": {
                          "type": "expr",
                          "config": {
                            "expression": "if({[default]YW_Demo/Config/TankCount} == 1, {'classes': 'button-primary', 'minWidth': '60px', 'padding': '12px'}, {'classes': 'button-secondary', 'minWidth': '60px', 'padding': '12px'})"
                          }
                        }
                      }
                    },
                    "props": {
                      "text": "1"
                    },
                    "type": "ia.input.button"
                  },
