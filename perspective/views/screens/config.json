{
  "custom": {},
  "params": {},
  "propConfig": {},
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "name": "Title"
        },
        "position": {
          "basis": "60px"
        },
        "props": {
          "text": "System Configuration",
          "style": {
            "classes": "screen-title"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "ConfigPanel"
        },
        "position": {
          "basis": "400px"
        },
        "props": {
          "style": {
            "classes": "card",
            "margin": "16px",
            "padding": "24px"
          }
        },
        "type": "ia.container.flex",
        "children": [
          {
            "meta": {
              "name": "SiteName"
            },
            "props": {
              "direction": "row",
              "alignItems": "center",
              "justify": "space-between",
              "style": {
                "padding": "12px"
              }
            },
            "type": "ia.container.flex",
            "children": [
              {
                "meta": {
                  "name": "Label"
                },
                "props": {
                  "text": "Site Name:",
                  "style": {
                    "classes": "label-primary",
                    "fontSize": "16px"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Input"
                },
                "propConfig": {
                  "text": {
                    "binding": {
                      "type": "tag",
                      "config": {
                        "tagPath": "[default]YW_Demo/Config/SiteName",
                        "bidirectional": true
                      }
                    }
                  }
                },
                "props": {
                  "style": {
                    "minWidth": "300px"
                  }
                },
                "type": "ia.input.textField"
              }
            ]
